{
  "name": "express-error-handling",
  "version": "1.0.0",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "ts-node-esm server.ts",
    "dev": "nodemon server.ts"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node-esm",
      "js": "ts-node-esm"
    },
    "ignore": [
      "data.json"
    ]
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.0",
    "nodemon": "^2.0.22",
    "typescript": "^5.2.2"
  }
}
